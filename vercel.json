{
  "version": 2,
  "builds":[
   {"src":"index.js",
   "use":"@vercel/node"
   }, {
    "src":"dist/**",
     "use": "@vercel/static" ,
     "config": {
      "distDir": "dist"
    } 
   }
  ],
  "routes": [
   {
     "src": "/",
     "dest": "dist/index.html" 
   },
   {
     "src": "/(.+)",
     "dest": "dist/$1" 
   }
  ]
}
