{
  "version": 2,
  "builds":[
   {"src":"index.js",
   "use":"@vercel/node"
   }, {
    "src":"dist/**",
     "use": "@vercel/static" ,
     "config": {
      "distDir": "dist"
    } 
   }
  ],
  "routes": [
    {
      "src":"/auth/login",
      "dest":"index.js"
    },
    {
      "src":"/auth/signUp",
      "dest":"index.js"
    },
    {
      "src":"/auth/forgot-password",
      "dest":"index.js"
    },
    {
      "src":"/auth/reset-password/(.*)",
      "dest":"index.js"
    },
   {
     "src": "/(.+)",
     "dest": "dist/$1" 
   },
   {
      "src": "/",
      "dest": "dist/index.html" 
    }
  ]
}
